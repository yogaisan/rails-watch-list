<div class="banner">
  <h1 class="text-center">LISTS</h1>
</div>


<div class="text-center">
  <ul class="list-unstyled">
    <% @lists.each do |list| %>
      <li><%= link_to list.name, list_path(list) %></li>
    <% end %>
  </ul>

<%= link_to "Add a new list", new_list_path, class: 'btn btn-primary' %>
</div>
